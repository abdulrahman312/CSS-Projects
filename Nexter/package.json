{
  "name": "nexter",
  "version": "1.0.0",
  "description": "Nexter Property Project",
  "main": "'",
  "scripts": {
    "watch:sass": "sass --watch sass/main.scss css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.css",
    "compress:css": "sass css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass compress:css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "live-server": "^1.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  }
}
